  <builders>
    <hudson.tasks.Shell>
      <command>#!/bin/bash -xe
# Exports.
export ASSET=&quot;EA31337 EA31337-Backtest-Releases $EA_VER $VER_NAME-Backtest ~/files/EA&quot;
export CLONE_REPO=https://$GITHUB_API_TOKEN@github.com/EA31337-Tester/EA31337-${VER_NAME}-Sets
export RUN_TEST=&quot;-t -v -x -I $INI_OPTS -c $BaseCurrency -p $Symbol -d $Deposit -s $Spread -y $Year -m $Months -D $Digits -b $BT_Source -e EA31337-$VER_NAME -S $SET_OPTS -E $EA_OPTS -f */&apos;$BASE_DIR&apos;/*.set -M $MT4_VER -i &apos;$BASE_REPO/$RULES_INIT_PATH&apos; -i \&quot;\$(find ~ -maxdepth 5 -name Strategy-${Strategy}.test.rule || echo FAILED)\&quot; -r &apos;$REPORT_NAME&apos; -O &apos;$BASE_REPO/$BASE_DIR/$SUBDIR&apos;&quot;

# Build.
vagrant up --no-provision --destroy-on-error --provider=aws
vagrant provision</command>
    </hudson.tasks.Shell>
