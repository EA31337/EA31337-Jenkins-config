        <propertiesContent>VER_NAME=Lite
SET_OPTS=&quot;VerboseInfo=1&quot;
EA_OPTS=&quot;maxdrawdown_enable=0&quot;
INI_OPTS=&quot;TestPeriod=${TestPeriod},TestModel=0&quot;
BASE_REPO=EA31337-Lite-Sets
REPORT_NAME=Strategy-${Strategy}_${Symbol}_${Deposit}${BaseCurrency}_${Spread}spread_${Digits}digits_${Year}-${TestPeriod}-${BT_Source}-test
VM_NAME=${REPORT_NAME}
INIT_SCRIPT=/vagrant/init_test.sh
RULES_INIT_PATH=_rules/Common/.init.rules.inc
BASE_DIR=${Symbol}/${Profile}/${Deposit}${BaseCurrency}/${Spread}-spread/${Digits}-digits/${Year}
SUBDIR=_strategies
PUSH_REPO=1</propertiesContent>
        <loadFilesFromMaster>false</loadFilesFromMaster>
