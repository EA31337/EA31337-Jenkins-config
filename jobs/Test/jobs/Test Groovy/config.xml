    <script>// @NonCPS
def printList(params) {
    def jobs = [:]
    println params
    params.split(&quot;,&quot;).each { param -&gt;
        println &quot;Param: ${param}&quot;
    }
}
text = &quot;Foo,Bar,Baz,Qux&quot;
